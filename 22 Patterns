#include<iostream>
using namespace std;
void pattern1(int n)
{
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<n;j++)
        {
            cout<<"* ";
        }
        cout<<endl;
    }
}
void pattern2(int n)
{
    for(int i=n;i>=0;i--)
    {
        for(int j=i;j<=n;j++)
        {
        cout<<"* ";
        }
        cout<<endl;
    }
}
void pattern3(int n)
{
    for(int i=1;i<=n;i++)
    {
        for(int j=1;j<=i;j++)
        {
        cout<<j;
        }
        cout<<endl;
    }
}
void pattern4(int n)
{
    for(int i=1;i<=n;i++)
    {
        for(int j=1;j<=i;j++)
        {
        cout<<i;
        }
        cout<<endl;
    }
}
void pattern5(int n)
{
    for(int i=1;i<=n;i++)
    {
        for(int j=i;j<=n;j++)
        {
        cout<<"* ";
        }
        cout<<endl;
    }
}
void pattern6(int n)
{
    for(int i=5;i>=0;i--)
    {
        for(int j=1;j<=i;j++)
        {
        cout<<j;
        }
        cout<<endl;
    }
}
void pattern7(int n)
{
    for(int i=n-1;i>=0;i--)
    {
        for(int j=i;j>=0;j--)
        cout<<"  ";
        for(int k=0;k<2*n-2*i-1;k++)
        {
            cout<<"* ";
        }
        cout<<endl;
    }
}
void pattern8(int n)
{
    for(int i=0;i<n;i++)
    {
        for(int j=i;j>0;j--)
        {
        cout<<"  ";
        }
        for(int k=0;k<2*n-2*i-1;k++)
        {
            cout<<"* ";
        }
        cout<<endl;
    }
}
void pattern9(int n)
{
    for(int i=n-1;i>=0;i--)
    {
        for(int j=i;j>=0;j--)
        cout<<"  ";
        for(int k=0;k<2*n-2*i-1;k++)
        {
            cout<<"* ";
        }
        cout<<endl;
    }
    for(int i=1;i<n;i++)
    {
        for(int j=i;j>=0;j--)
        {
        cout<<"  ";
        }
        for(int k=0;k<2*n-2*i-1;k++)
        {
            cout<<"* ";
        }
        cout<<endl;
    } 
}
void pattern10(int n)
{
    for(int i=0;i<n;i++)
    {
        for(int j=i;j>=0;j--)
        {
            cout<<"* ";
        }
        cout<<endl;
    }
    for(int i=n-2;i>=0;i--)
    {
        for(int j=i;j>=0;j--)
        {
            cout<<"* ";
        }
        cout<<endl;
    }
}
void pattern11(int n)
{
    for(int i=0;i<n;i++)
    {
        if(i%2==0)
        {
            int start=1;
            for(int j=i;j>=0;j--)
            {
                cout<<start<<" ";
                start=1-start;
            }
            cout<<endl;
        }
        else
        {
            int start=0;
            for(int j=i;j>=0;j--)
            {
                cout<<start<<" ";
                start=1-start;
            }
            cout<<endl;
        }
    }
}
void pattern12(int n)
{
    for(int i=1;i<=n;i++)
    {
        for(int l=1;l<=i;l++)
        {
            cout<<l<<" ";
        }
        for(int j=0;j<2*n-2*i;j++)
        {
            cout<<"  ";
        }
        for(int k=i;k>=1;k--)
        {
            cout<<" "<<k;
        }
        cout<<endl;
    }
}
void pattern13(int n)
{
    int p=1;
    for(int i=0;i<n;i++)
    {
        for(int j=i;j>=0;j--)
        {
            cout<<p++<<" ";
        }
        cout<<endl;
    }
}
void pattern14(int n)
{
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<=i;j++)
        {
            char ch='A'+j;
            cout<<ch<<" ";
        }
        cout<<endl;
    }
}
void pattern15(int n)
{
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<n-i;j++)
        {
            char ch='A'+j;
            cout<<ch<<" ";
        }
        cout<<endl;
    }
}
void pattern16 (int n)
{
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<=i;j++)
        {
            char ch='A'+i;
            cout<<ch;
        }
        cout<<endl;
    }
}
